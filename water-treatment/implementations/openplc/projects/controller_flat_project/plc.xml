dbg_Alarms_LL_alarm := True;
(* if the level is at 500 or lower than 500 open the valve *)
elsif dbg_P1_RW_Tank_tnk_lvl &lt;= 500.0 and dbg_P1_SYS_RUNNING then
(* output to turn the valve open *)
dbg_P1_RW_Tank_PR_Valve := True;
dbg_Alarms_L_alarm := True;
(* if level is at 800 *)
elsif  dbg_P1_RW_Tank_tnk_lvl = 800.0 and dbg_P1_SYS_RUNNING then
dbg_P1_RW_Tank_PR_Valve := False; (* close the valve *)
dbg_Alarms_H_alarm := True;
elsif dbg_P1_RW_Tank_tnk_lvl = 1200.0 and dbg_P1_SYS_RUNNING then
dbg_Alarms_HH_alarm := True; (* if the tank is higher than or equal to 1200 set the high high alarm *)
end_if;

(* Raw_water_pump *)
if dbg_P3_Ultrafiltration_UFFT_Tank_tnk_lvl = 800.0 AND dbg_P1_SYS_RUNNING then
(* start the pump and open the valve from the other tank *)
dbg_P1_RW_Tank_P_Valve := True;
dbg_P1_RW_Pump_start := True;
dbg_P1_RW_Pump_stop := False;
elsif dbg_P3_Ultrafiltration_UFFT_Tank_tnk_lvl = 1000.0 AND dbg_P1_SYS_RUNNING then
(* stop the pump and close the valve *)
dbg_P1_RW_Tank_P_Valve := False;
dbg_P1_RW_Pump_stop := True;
dbg_P1_RW_Pump_start := False;
end_if;

(* State_change *)
if not dbg_HMI_Start_Active AND not dbg_HMI_Stop_Active then
(* if start and stop is not pressed go to idle *)
dbg_P1_SYS_IDLE := True;
dbg_P1_SYS_START := False;
dbg_P1_SYS_RUNNING := False;
dbg_P1_SYS_SHUTDOWN := False;
elsif  dbg_HMI_Start_Active AND not dbg_HMI_Stop_Active then
(* if the start pb and stop is not pressed then change the state of process one to starting *)
dbg_P1_SYS_IDLE := False;
dbg_P1_SYS_START := True;
dbg_P1_SYS_RUNNING := False;
dbg_P1_SYS_SHUTDOWN := False;
(* if the tank level is greater than 250 at the start and UFFT is less than a 1000 we can start *)
(* then and all permissves cond together into one tag so we know the plant is ready to start *)
dbg_P1_SYS_Permissives_Ready := dbg_P1_SYS_Permissives[0] AND dbg_P1_SYS_Permissives[1];
elsif dbg_HMI_Start_Active AND dbg_P1_SYS_Permissives_Ready then
(* if the start was pressed and permissives are ready start running Process one *)
dbg_P1_SYS_IDLE := False;
dbg_P1_SYS_START := False;
dbg_P1_SYS_RUNNING := True;
dbg_P1_SYS_SHUTDOWN := False;
elsif dbg_HMI_Stop_Active then
(* if the stop button is pressed then go to shutdown state *)
dbg_P1_SYS_IDLE := False;
dbg_P1_SYS_START := False;
dbg_P1_SYS_RUNNING := False;
dbg_P1_SYS_SHUTDOWN := True;
(* set all permissives to false *)
dbg_P1_SYS_Permissives_Ready := False;
end_if;
